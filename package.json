{
  "name": "pdie2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "./build.sh",
    "build-server": "babel-node scripts/build-server.js",
    "client": "babel-node scripts/start.js",
    "dev": "npm-run-all --parallel client server",
    "dev:server": "npm-run-all --parallel watch:server run:server",
    "run:server": "nodemon --exec node public/server-bundle.js",
    "server": "nodemon --exec babel-node -- src/server.js",
    "start": "node src/server.js",
    "test": "babel-node scripts/test.js",
    "test-cov": "jest --coverage",
    "test-watch": "jest --watch",
    "watch:server": "babel-node scripts/watch-server.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^1.1.1",
    "@google-cloud/vision": "^0.11.2",
    "aws-sdk": "^2.60.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "gcloud": "^0.37.2",
    "google-cloud": "^0.54.0",
    "googleapis": "^19.0.0",
    "material-ui": "^0.17.4",
    "prop-types": "^15.5.6",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-grid-gallery": "^0.3.2",
    "react-redux": "^5.0.4",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^4.0.2",
    "css-loader": "^0.26.1",
    "deep-freeze": "0.0.1",
    "detect-port": "^1.1.0",
    "dotenv": "^2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.24.0",
    "http-proxy-middleware": "^0.17.3",
    "isomorphic-style-loader": "^2.0.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dev-utils": "^0.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "^2.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
